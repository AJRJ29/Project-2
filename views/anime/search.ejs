<div>
    <form method="GET" action="/">
        <input type="text" name="title">
        <button class="btn btn-primary" type="submit">Search</button>
    </form>

 <% anime.forEach(function(anime) { %>
        <div class="well">
        <h2><a href="/anime/<%= anime.title %>"><%= anime.title %></a></h2>
        </div>
    <% }); %> 
</div>